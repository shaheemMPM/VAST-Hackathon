{
  "manifest_version": 2,
  "name": "digital wellbeing",
  "version": "1.0",
  "description": "Great technology should improve life, not distract from it.",
  "icons":{
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "chrome_url_overrides" : {
    "newtab": "myPage.html"
  },
  "permissions": [
      "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
      ],
      "js": ["intercept.js"]
    },
    {
      "matches": [
        "http://www.twitter.com/*",
        "https://www.twitter.com/*",
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": ["twitter.js"]
    },
    {
      "matches": [
        "http://www.twitter.com/*",
        "https://www.twitter.com/*",
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": ["twitter.js"]
    },
    {
      "matches": [
        "http://www.instagram.com/*",
        "https://www.instagram.com/*",
        "http://instagram.com/*",
        "https://instagram.com/*"
      ],
      "js": ["instagram.js"]
    }
  ],
  "browser_action":{
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "click to customize settings"
  }
}
