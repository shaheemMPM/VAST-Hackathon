{
   "name": "Digital Wellbeing",
   "version": "1.4.2",
   "description": "Great technology should improve life, not distract from it.",
   "manifest_version": 2,
   "background": {
      "scripts": [ "js/config.js", "js/fn.js", "js/storage-local.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon16.png",
      "default_popup": "popup.html",
      "default_title": "Digital Wellbeing"
   },
  "chrome_url_overrides" : {
    "newtab": "myPage.html"
  },
  "content_scripts": [
      {
        "matches": [
          "http://www.facebook.com/*",
          "https://www.facebook.com/*"
        ],
        "js": ["intercept.js"]
      },
      {
        "matches": [
          "http://www.twitter.com/*",
          "https://www.twitter.com/*",
          "http://twitter.com/*",
          "https://twitter.com/*"
        ],
        "js": ["twitter.js"]
      },
      {
        "matches": [
          "http://www.twitter.com/*",
          "https://www.twitter.com/*",
          "http://twitter.com/*",
          "https://twitter.com/*"
        ],
        "js": ["twitter.js"]
      },
      {
        "matches": [
          "http://www.instagram.com/*",
          "https://www.instagram.com/*",
          "http://instagram.com/*",
          "https://instagram.com/*"
        ],
        "js": ["instagram.js"]
      }
    ],
   "icons":{
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
   },
   "key": "",
   "offline_enabled": true,
   "optional_permissions": [ "\u003Call_urls>" ],
   "permissions": [ "tabs", "idle", "storage", "unlimitedStorage"]
}
